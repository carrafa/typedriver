<script type="text/x-handlebars-template" id="handlebars-template">
  <table id="simpleTable">
    <thead>
      <tr>
        <th data-sort="string">name</th>
        <th data-sort="string">sentence</th>
        <th data-sort="string">time</th>
        <th data-sort="float">wpm</th>
        <th data-sort="float">cpm</th>
        <th data-sort="integer">place</th>
      </tr>
    </thead>

    <tbody>
      {{#each finishers}}
      <tr>
        <td class="username">{{username}}</td>
        <td class="sentence">{{sentence}}</td>
        <td class="time">{{time}}</td>
        <td class="wpm">{{wpm sentence time}}</td>
        <td class="cpm">{{cpm sentence time}}</td>
        <td class="place">{{place raceId}}</td>
      </tr>
      {{/each}}
    </tbody>

  </table>
</script>
